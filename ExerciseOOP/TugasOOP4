//Problem : Perform addition operation on complex data using class and object. The program should ask for real and
            imaginary part of two complex numbers, and display the real and imaginary parts of their sum.
//Solution :

#include <iostream>
using namespace std;

class Imagin{
	public:

	double x,y;

	Imagin(double ax, double ay){

	x=ax;
	y=ay;}
};

int main(){

	double an1,an2;
	cout << "Angka pertama \n";
	cout<< "Angka Origin:";
	cin>> an1;
	cout<< "Angka Imagin:";
	cin>> an2;
	Imagin angka1(an1,an2);
	cout << "Angka kedua \n";

    double an3,an4;
	cout<< "Angka Origin:";
	cin>> an3;
	cout<< "Angka Imagin:";
	cin>> an4;
	Imagin angka2(an3,an4);

	double Origin;
    Origin = angka1.x +angka2.x;

    double Imagin;
    Imagin = angka2.x + angka1.y;

    cout << "Jumla angka Origin "<< Origin << "\n";
    cout << "Jumlah angka Imagin "<< Imagin;

    return 0;
}

  
   
